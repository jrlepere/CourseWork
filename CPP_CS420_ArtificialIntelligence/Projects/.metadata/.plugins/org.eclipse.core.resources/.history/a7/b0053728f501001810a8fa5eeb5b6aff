import java.util.List;

/**
 * A search algorithm interface
 * @author JLepere2
 * @date 01/25/2018
 */
public interface ISearchAlgorithm {
	
	/**
	 * Searches for a list actions to solve the problem
	 * @param p the problem
	 * @return a list of actions
	 */
	public List<IAction> search(Problem p);
	
}
